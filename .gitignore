# Ignore the vendor directory (for Composer dependencies if you're using Composer)
vendor/

# Ignore environment configuration files (e.g., .env)
.env
.env.local
.env.*.local

# Ignore PHP session files (temporary files)
session/

# Ignore IDE/editor specific files (e.g., VSCode, PHPStorm, etc.)
.vscode/
.idea/

# Ignore OS-specific files
.DS_Store      # macOS
Thumbs.db      # Windows

# Ignore log files and database dumps
*.log
*.sql
*.sqlite

# Ignore uploaded files (e.g., receipts, images, etc.)
uploads/

# Ignore backup files (e.g., editor backup files)
*~

# Ignore temporary PHP files (e.g., generated by editors or servers)
*.php~
